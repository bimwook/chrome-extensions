{
   "background": {
      "scripts": [  ]
   },
   "browser_action": {
      "default_icon": "icon16.png",
      "default_title": "weibo"
   },
   "content_scripts": [ {
      "css": [ "normal.css" ],
      "js": [ "jquery.js", "content.js" ],
      "matches": ["https://weibo.com/*", "https://s.weibo.com/*", "https://d.weibo.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "weibo saver",
   "icons": {
      "16": "icon16.png",
      "128": "icon128.png"
   },
   "manifest_version": 2,
   "name": "weibo",
   "permissions": [ "activeTab", "http://*/*", "https://*/*" ],
   "version": "1.0"
}
